{
    "name": "hearstuk/zf1-component-uri",
    "description": "The Zend_Uri component from Zend Framework 1.12 abstracted out for use with composer.",
    "homepage": "http://github.com/joegreen88/zf1-component-validate",
    "authors": [
        {
            "name": "Joe Green",
            "email": "Joe.Green@hearst.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "hearstuk/zf1-components-base": "~1.2",
        "hearstuk/zf1-component-validate": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.20"
    },
    "autoload": {
        "classmap": ["src/Zend/Uri.php"],
        "psr-0": {
            "Zend_Uri": "src/",
			"Tests\\Zend\\Uri": ""
        }
    }
}